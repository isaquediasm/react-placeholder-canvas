{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    ["@babel/plugin-transform-runtime"],
    ["@babel/plugin-transform-async-to-generator"],    
    [
      "babel-plugin-styled-components",
      {
        "displayName": false,
        "pure": true
      }
    ],
    ["module-resolver", {
      "root": ["./src"]
    }]
  ]
}