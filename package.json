{
  "name": "react-placeholder-canvas",
  "version": "0.0.1",
  "description": "Boilerplate project for kickstarting development on a new project",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "webpack-dev-server --open --mode development",
    "build": "yarn clean && yarn build-es",
    "build-es": " NODE_ENV=production babel src -d dist --source-maps inline --ignore src/**/*.test.js,src/**/*.story.js",
    "clean": "rimraf dist",
    "dev": "webpack --mode development",
    "lint": "eslint src --max-warnings=0",
    "fixformat": "eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --colors",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/isaquediasm/react-placeholder-canvas.git"
  },
  "author": "Isaque Dias",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/isaquediasm/react-placeholder-canvas/issues"
  },
  "husky": {
    "hooks": {}
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "pretty-quick --staged",
      "eslint src/ --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/plugin-transform-async-to-generator": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.4",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-macros": "^2.4.2",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "css-loader": "^3.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^6.2.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jest": "^22.5.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "jest-styled-components": "^6.3.1",
    "react-hot-loader": "^4.8.4",
    "react-test-renderer": "^16.10.1",
    "rimraf": "^2.6.3",
    "semantic-release": "^15.13.24",
    "style-loader": "^1.0.0",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.5",
    "acorn": "^7.1.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "styled-components": "^4.2.0",
    "styled-theming": "^2.2.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "homepage": "https://github.com/isaquediasm/react-placeholder-canvas#readme"
}